
@{
    Layout = null;
}
 
<!DOCTYPE html>
 
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
    <h1>
    @{
        if (ViewBag.Status==false)
        {
            @ViewBag.Message
            return;
        }
    }
    </h1>

    <h1>@ViewBag.Question</h1>
    <ul>
        @{
            for(int i =0; i< ViewBag.AnswersList.Count; i++)
            {
                <li><a href='/Answer?number=@ViewBag.answersListID[i]'>@ViewBag.answersList[i]</a></li>
            }
        }
    </ul>
</body>
</html>
